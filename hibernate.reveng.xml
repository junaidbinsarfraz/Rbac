<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>
  <table-filter match-catalog="rbac" match-name="acess_type"/>
  <table-filter match-catalog="rbac" match-name="permission"/>
  <table-filter match-catalog="rbac" match-name="resource"/>
  <table-filter match-catalog="rbac" match-name="role"/>
  <table-filter match-catalog="rbac" match-name="role_permission"/>
  <table-filter match-catalog="rbac" match-name="user"/>
  <table-filter match-catalog="rbac" match-name="user_role"/>
</hibernate-reverse-engineering>